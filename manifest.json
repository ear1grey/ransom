{
  "manifest_version": 3,
  "name": "Ransome Note Extension",
  "version": "1.0",
  "description": "A Silly Interface Hackday creation",
  "icons": {
    "16": "i/icon16.png",
    "48": "i/icon48.png",
    "128": "i/icon128.png"
  },
  "content_scripts": [
    {
     "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "css" : ["ransom.css"]
     }
  ],
  "permissions": ["activeTab"]
}
